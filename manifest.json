{
  "manifest_version": 3,
  "name": "Phone Number Inserter",
  "version": "1.0",
  "description": "Insert my phone number into text fields",
  "permissions": ["activeTab", "scripting"],
  "commands": {
    "insert-phone": {
      "suggested_key": {
        "default": "Ctrl+Shift+P"
      },
      "description": "Insert phone number"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}

